{
  "private": true,
  "type": "module",
  "scripts": {
    "start": "bin/dev",
    "build": "vite build && find ./app/assets/javascripts/generated/ -type f -print0 | xargs -0 dos2unix",
    "dev": "vite",
    "lint:js": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:js:fix": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:css": "stylelint \"**/*.{scss,css}\" --formatter compact",
    "lint:css:fix": "stylelint \"**/*.{scss,css}\" --fix --formatter compact",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "rubocop": "bundle exec rubocop -A",
    "lint": "npm run lint:js && npm run lint:css",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.jsx": "eslint --fix",
    "*.ts": "eslint --fix",
    "*.tsx": "eslint --fix",
    "*.scss": "stylelint --fix",
    "*.css": "stylelint --fix",
    "*.rb": "bundle exec rubocop -A"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.1",
    "@prettier/plugin-ruby": "^1.6.1",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-stylelint": "^22.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "stylelint": "^16.21.0",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-config-twbs-bootstrap": "^16.1.0",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.12.1",
    "vite": "^6.3.5",
    "vite-plugin-devtools-json": "^0.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-rails": "^0.5.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@popperjs/core": "^2.11.8",
    "@reduxjs/toolkit": "^2.8.2",
    "@sentry/react": "^9.32.0",
    "axios": "^1.10.0",
    "bootstrap": "^5.3.7",
    "modern-normalize": "^3.0.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.8.1",
    "qs": "^6.14.0",
    "rc-slider": "^11.1.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  }
}
